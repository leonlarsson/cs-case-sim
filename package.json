{
  "name": "cs2-case-ui-react",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "drizzle-kit push && next start",
    "lint": "next lint",
    "prettier": "prettier . --write",
    "db:push": "drizzle-kit push",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:delete-old-non-converts": "node --experimental-strip-types --env-file=.env  ./db/scripts/deleteOldNonCoverts.ts",
    "db:insert-cases": "node --experimental-strip-types --env-file=.env  ./db/scripts/insertCasesToDB.ts",
    "db:insert-items": "node --experimental-strip-types --env-file=.env  ./db/scripts/insertItemsToDB.ts",
    "db:seed": "npm run db:insert-cases && npm run db:insert-items"
  },
  "dependencies": {
    "@planetscale/database": "^1.19.0",
    "@vercel/functions": "^1.4.1",
    "better-sqlite3": "^11.3.0",
    "drizzle-orm": "^0.33.0",
    "howler": "^2.2.4",
    "next": "^14.2.7",
    "react": "^18",
    "react-dom": "^18",
    "sharp": "^0.33.5",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.11",
    "@types/howler": "^2.2.11",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10",
    "drizzle-kit": "^0.22.8",
    "postcss": "^8",
    "prettier": "^3.3.0",
    "prettier-plugin-tailwindcss": "^0.6.1",
    "tailwindcss": "^3.4.10",
    "typescript": "^5"
  }
}
